<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Expedite Suite" preserve-order="true">
    <listeners>
        <listener class-name="com.tomash.gombosh.web.listeners.TestListener"/>
    </listeners>

    <test name="Login Tests">
        <classes>
            <class name="com.tomash.gombosh.web.tests.LoginTest"/>
        </classes>
    </test>

    <test name="Logout Tests">
        <classes>
            <class name="com.tomash.gombosh.web.tests.LogoutTest"/>
        </classes>
    </test>
</suite>